{
  "name": "cereza",
  "version": "0.1.0",
  "description": "Local directory browser",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "apollo": "vue-cli-service apollo:dev --generate-schema",
    "apollo:schema:generate": "vue-cli-service apollo:schema:generate",
    "apollo:schema:publish": "vue-cli-service apollo:schema:publish",
    "apollo:start": "vue-cli-service apollo:start",
    "build:server": "tsc --project tsconfig.server.json && node ./tools/copyServerFiles.js"
  },
  "dependencies": {
    "audio-extensions": "0.0.0",
    "classnames": "^2.3.2",
    "core-js": "^3.25.5",
    "dotenv": "^16.0.3",
    "graphql-type-json": "^0.3.2",
    "image-extensions": "^1.1.0",
    "merge-graphql-schemas": "^1.7.8",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.7.2",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.5.1",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.5",
    "umzug": "^2.3.0",
    "video-extensions": "^1.2.0",
    "vue": "^2.7.13",
    "vue-apollo": "^3.1.0",
    "vue-class-component": "^7.2.6",
    "vue-meta": "^2.4.0",
    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@types/sequelize": "^4.28.14",
    "@types/umzug": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vue/cli-plugin-babel": "~4.4.6",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-pwa": "~4.4.6",
    "@vue/cli-plugin-router": "~4.4.6",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "cpy": "^9.0.1",
    "eslint": "^8.25.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^6.2.2",
    "graphql-tag": "^2.12.6",
    "lint-staged": "^13.0.3",
    "sass": "^1.55.0",
    "prettier": "^2.7.1",
    "sass-loader": "^13.1.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "~4.8.4",
    "vue-cli-plugin-apollo": "^0.22.2",
    "vue-template-compiler": "^2.7.13"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "{,src,interfaces,apollo-server}/**/*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint --fix",
      "git add"
    ]
  }
}
